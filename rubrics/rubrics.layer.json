{
	"child":{
		"seojsontpl":"*{infra.conf.rubrics.main}/{istate.name}.json",
		"tpl":["<h1>{istate.name}</h1><p>{infra.conf.rubrics.404}</p><p><a href='?'>{infra.conf.rubrics.link}</a></p>"],
		"tplroot":"root",
		"autoedittpl":{"fast":"*{infra.conf.rubrics.main}/{istate.name}"},
		"showanimate":true,
		"layers":{
			"tplcheck":true,
			"autoedittpl":{"fast":"*{infra.conf.rubrics.main}/{istate.name}"},
			"tpltpl":"*rubrics/rubrics.php?type={infra.conf.rubrics.main}&id={istate.name}&show"
		}
	},
	"childs":{
		"info":{
			"seojsontpl":"*{istate.name}/{istate.name}.json",
			"tpl":["<div id='rubricstitle'></div><div id='rubricscontent'></div>"],
			"tplroot":"root",
			"autoedittpl":{"fast":"*{istate.name}/"},
			"divs":{
				"rubricstitle":{
					"tpl":["<h1>{infra.conf.rubrics.list[istate.name].title}</h1>"],
					"tplroot":"root"
				},
				"rubricscontent":{
					"jsontpl":"*rubrics/rubrics.php?type={istate.name}&list&lim=0,50",
					"tpl":"*rubrics/rubrics.tpl",
					"tplroot":"PAGES",
					"child":{
						"showanimate":true,
						"tpltpl":"*rubrics/rubrics.php?type={istate.parent.name}&show&id={istate.name}",
						"autoedittpl":{ "fast":"*{istate.parent.name}/{istate.name}" },
						"divs":{
							"rubricstitle":{
								"tpl":[""]
							}
						}
					}
				}
			}
		},
		"files":{
			"seojsontpl":"*seo/{istate.name}.json",
			"tpl":["<div id='rubricstitle'></div><div id='rubricscontent'></div>"],
			"tplroot":"root",
			"autoedittpl":{"fast":"*{istate.name}/"},
			"divs":{
				"rubricstitle":{
					"tpl":["<h1>{infra.conf.rubrics.list[istate.name].title}</h1>"],
					"tplroot":"root"
				},
				"rubricscontent":{
					"jsontpl":"*rubrics/rubrics.php?type={istate.name}&list&lim=0,50",
					"tpl":"*rubrics/rubrics.tpl",
					"tplroot":"FILES"
				}
			}
		},
		"list":{
			"tpl":["<div id='rubricstitle'></div><div id='rubricscontent'></div>"],

			"autoedittpl":{"fast":"*{istate.name}/"},
			"divs":{
				"rubricstitle":{
					"tpl":["<h1>{infra.conf.rubrics.list[istate.name].title}</h1>"],
					"tplroot":"root"
				},
				"rubricscontent":{
					"tpl":"*rubrics/rubrics.tpl",
					"tplroot":"PAGES",
					"jsontpl":"*rubrics/rubrics.php?type={istate.name}&list&lim=0,50",
					"child":{
						"showanimate":true,
						"tpltpl":"*rubrics/rubrics.php?type={istate.parent.name}&show&id={istate.name}",
						"autoedittpl":{ "fast":"*{istate.parent.name}/{istate.name}" },
						"divs":{
							"rubricstitle":{
								"tpl":["<a href='?{istate.parent.name}'>{infra.conf.rubrics.list[istate.parent.name].title}</a>"],
								"tplroot":"root"
							}
						}
					}
				}
			}
		}
	}
}
